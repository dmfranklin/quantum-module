        <statement>
          <p>⚪ <m>\ket{0}</m>
          </p>
        </statement>
      </choice>
      <choice correct="yes">
        <statement>
          <p>⚫ <m>\ket{1}</m>
          </p>
        </statement>
      </choice>
    </choices>
  </exercise>

