  <exercise label="HW1-Q1">
    <title>Question 1</title>
    <idx>question 1</idx>
    <statement>
      <p>Select all of the TRUE statements about quantum circuits.</p>
    </statement>
    <choices randomize="yes">
      <choice correct="yes">
        <statement>
          <p>The order of operations goes from left to right.</p>
        </statement>
      </choice>
      <choice correct="yes">
        <statement>
          <p>A quantum circuit may contain one or more operations.</p>
        </statement>
      </choice>
      <choice>
        <statement>
          <p>A quantum circuit depicts the physical movement of a qubit.</p>
        </statement>
      </choice>
      <choice>
        <statement>
          <p>Each quantum operation has one input and one output.</p>
        </statement>
      </choice>
    </choices>
  </exercise>

